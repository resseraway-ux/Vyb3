# Copy to .env.local (do NOT commit .env.local)
# NFT.Storage API key to upload post content to IPFS
NEXT_PUBLIC_NFT_STORAGE_KEY=your_nft_storage_api_key_here

# Optional: RPC for Hardhat / network deploys
NEXT_PUBLIC_RPC_URL=https://eth-rinkeby.alchemyapi.io/v2/your_key

# Private key for contract deploy (use a deploy account; NEVER commit)
PRIVATE_KEY=0x...

# CONTRACT_ADDRESS - set after deployment if you want the frontend to read it
NEXT_PUBLIC_CONTRACT_ADDRESS=